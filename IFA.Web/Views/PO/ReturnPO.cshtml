                                        <label class="col-md-2 col-form-label">Status</label>
                                        <div class="col-md-10">
                                            <input class="form-control" value="" id="Status" disabled />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="GridDetails"></div>
